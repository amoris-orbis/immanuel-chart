{
    "name": "sunlight/immanuel-chart",
    "description": "Lumen package - bridge to a Python astrology script powered by Flatlib.",
    "type": "library",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Robert Davies",
            "email": "rob@sunlightdigital.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sunlight\\ImmanuelChart\\": "src"
